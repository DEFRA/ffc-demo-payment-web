{
  "consumer": {
    "name": "ffc-demo-payment-web"
  },
  "provider": {
    "name": "ffc-demo-payment-service"
  },
  "interactions": [
    {
      "description": "get all schedules",
      "providerState": "schedules exist",
      "request": {
        "method": "GET",
        "path": "/schedule"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "claimId": "MINE123"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          },
          "$.body[*].claimId": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}